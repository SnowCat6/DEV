<Styles>
	<Style name="Заголовок" element="h2">
		<Attribute name="class" value="title" />
	</Style>
	<Style name="Разделитель" element="hr">
		<Attribute name="size"  value="1" />
		<Attribute name="clear" value="both" />
	</Style>

	<Style name="Иконка 1" element="div">
		<Attribute name="class" value="iconStyle" />
	</Style>
	<Style name="Иконка 2" element="div">
		<Attribute name="class" value="iconStyle icon2" />
	</Style>
	<Style name="Иконка 3" element="div">
		<Attribute name="class" value="iconStyle icon3" />
	</Style>
	<Style name="Иконка 4" element="div">
		<Attribute name="class" value="iconStyle icon4" />
	</Style>
	<Style name="Иконка 5" element="div">
		<Attribute name="class" value="iconStyle icon5" />
	</Style>
	<Style name="Иконка 6" element="div">
		<Attribute name="class" value="iconStyle icon6" />
	</Style>
	<Style name="Иконка 7" element="div">
		<Attribute name="class" value="iconStyle icon7" />
	</Style>
	<Style name="Иконка 8" element="div">
		<Attribute name="class" value="iconStyle icon8" />
	</Style>
	<Style name="Иконка 9" element="div">
		<Attribute name="class" value="iconStyle icon9" />
